<% title t('.page_title') %>

<div class="grid-row">
  <div class="column-two-thirds">
    <%= step_header %>

    <h1 class="heading-xlarge gv-u-heading-xxlarge"><%=t '.heading' %></h1>

    <%= step_form @form_object do |f| %>
      <%= f.radio_button_fieldset :other_children, inline: true, choices: GenericYesNo.values %>

      <%= f.submit class: 'button' %>
    <% end %>
  </div>
</div>
